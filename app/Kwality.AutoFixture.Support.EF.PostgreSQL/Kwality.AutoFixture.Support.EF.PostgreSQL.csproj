<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Title>Kwality.AutoFixture.Support.EF.PostgreSQL</Title>
    <Version>2.0.0</Version>
    <Authors>kdeconinck</Authors>
    <Description>PostgreSQL support for AutoFixture.</Description>
    <PackageProjectUrl>https://github.com/dotnet-essentials/Kwality.AutoFixture.Support.EF.PostgreSQL</PackageProjectUrl>
    <RepositoryUrl>https://github.com/dotnet-essentials/Kwality.AutoFixture.Support.EF.PostgreSQL.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AutoFixture" Version="4.18.0" />
    <PackageReference Include="JetBrains.Annotations" Version="2022.3.1" />
    <PackageReference Include="Kwality.CodeStyle" Version="2.0.20" />
    <PackageReference Include="Npgsql.EntityFrameworkCore.PostgreSQL" Version="7.0.3" />
  </ItemGroup>

</Project>
